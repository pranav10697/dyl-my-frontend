pipeline{
    agent any
    stages{
        stage('pull'){
            steps{
               git branch: 'dev', url: 'https://github.com/pranav10697/dyl-my-frontend.git' 
            }
        }
        stage('build'){
            steps{
            sh '''npm install
                  ng build'''
            }
        }
        stage('deploy'){
            steps{
                sh ' usr/local/bin/aws cp --recursive dist/angular-frontend/ s3://super-mario12/ '
            }
        }
    }
}